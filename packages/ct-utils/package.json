{
  "name": "@mikesprague/ct-utils",
  "version": "0.1.1",
  "description": "Utilities and helper methods",
  "type": "module",
  "engines": {
    "node": ">= 24.x",
    "npm": ">= 11.x"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "volta": {
    "extends": "../../package.json"
  },
  "scripts": {
    "biome": "npx biome lint .",
    "build": "npx tsdown",
    "clean": "npx rimraf dist",
    "docs": "documentation readme ./src/index.ts --section=API",
    "dev": "npx tsdown --watch",
    "postbuild": "npm run docs",
    "prebuild": "npm run clean",
    "predev": "npm run clean",
    "typecheck": "npx tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikesprague/packages-for-work.git",
    "directory": "packages/ct-utils"
  },
  "author": {
    "name": "Michael Sprague",
    "email": "mikesprague@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mikesprague/packages-for-work/issues"
  },
  "homepage": "https://github.com/mikesprague/packages#readme"
}
